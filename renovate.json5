{
  "extends": [
    // Base config - https://github.com/giantswarm/renovate-presets/blob/main/default.json5
    "github>giantswarm/renovate-presets:default.json5"
  ],
  "regexManagers": [
    {
      "fileMatch": ["test/hack/bin/fetch-tools.sh"],
      "matchStrings": ["ARCHITECT_VERSION=\"(?<currentValue>.*?)\""],
      "depName": "giantswarm/architect",
      "datasource": "github-releases"
    },
    {
      "fileMatch": ["test/hack/bin/fetch-tools.sh"],
      "matchStrings": ["HELM_VERSION=\"(?<currentValue>.*?)\""],
      "depName": "helm/helm",
      "datasource": "github-releases"
    },
    {
      "fileMatch": ["test/hack/bin/fetch-tools.sh"],
      "matchStrings": ["JQ_VERSION=\"(?<currentValue>.*?)\""],
      "depName": "jqlang/jq",
      "datasource": "github-releases"
    },
    {
      "fileMatch": ["test/hack/bin/fetch-tools.sh"],
      "matchStrings": ["LOKITOOL_VERSION=\"(?<currentValue>.*?)\""],
      "depName": "grafana/loki",
      "datasource": "github-releases"
    },
    {
      "fileMatch": ["test/hack/bin/fetch-tools.sh"],
      "matchStrings": ["PINT_VERSION=\"(?<currentValue>.*?)\""],
      "depName": "cloudflare/pint",
      "datasource": "github-releases"
    },
    {
      "fileMatch": ["test/hack/bin/fetch-tools.sh"],
      "matchStrings": ["PROMETHEUS_VERSION=\"(?<currentValue>.*?)\""],
      "depName": "prometheus/prometheus",
      "datasource": "github-releases"
    },
    {
      "fileMatch": ["test/hack/bin/fetch-tools.sh"],
      "matchStrings": ["YQ_VERSION=\"(?<currentValue>.*?)\""],
      "depName": "mikefarah/yq",
      "datasource": "github-releases"
    }
  ]
}
